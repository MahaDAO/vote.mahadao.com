{
  "name": "mkr-governance-portal",
  "type": "static",
  "alias": "dai-gov-staging",
  "version": 1,
  "static": {
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "Deny"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "X-XSS-protection",
            "value": "1; mode=block"
          }
        ]
      }
    ]
  }
}
